@model List<RoomDetail>
@{
    Layout = null;
}

<div class="gallery padding-top padding-bottom">
    <div class="container">
        @foreach (var item in Model)
        {
            <div class="gallery__wrapper">
                <div class="row g-4">
                    <div class="col-md-4">
                        <div class="gallery__grid-item">
                            <div class="gallery__grid-inner">
                                <div class="gallery__grid-image">
                                    <img src="@item.ImageUrl2" alt="gallery image">
                                    <a href=" @item.ImageUrl" data-rel="lightcase" class="gallery__view-btn">
                                        <i class="fa-solid fa-expand"></i>
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        }
        
    </div>
</div>
