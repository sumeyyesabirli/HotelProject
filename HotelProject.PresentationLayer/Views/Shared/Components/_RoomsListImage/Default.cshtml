@model List<Room>
@{
    Layout = null;
}



<div class="container flex flex-row justify-content-center align-content-center">
                @foreach (var item in Model)
                {
        
                        <div class="gallery__grid-item">
                            <div class="gallery__grid-inner">
                                <div class="gallery__grid-image">
