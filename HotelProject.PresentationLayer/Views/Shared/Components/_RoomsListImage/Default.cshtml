                                    <a href="@item.ImageUrl" data-rel="lightcase" class="gallery__view-btn">
                                        <i class="fa-solid fa-expand"></i>
                                    </a>
                                </div>
                            </div>
                        </div>

                        <div class="gallery__grid-item">
