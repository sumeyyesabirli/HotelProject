@model HotelProject.Entitylayer.Concrete.Staff
@{
    ViewData["Title"] = "AddStaff";
    Layout = "~/Views/Shared/_Admin1Layout.cshtml";
}

<form method="post">
    <label>Personel Adı</label>
    <input type="text" asp-for="StaffName" class="form-control" />
    <br />
    <label>Personel Soyadı</label>
    <input type="text" asp-for="StaffSurname" class="form-control" />
    <br />
    <label>Email</label>
    <input type="text" asp-for="StaffEmail" class="form-control" />
    <br>
    <label>Personel Ünvanı</label>
    <input type="text" asp-for="StaffTitle" class="form-control" />
    <br />
    <label>Telefon Numarası</label>
    <input type="text" asp-for="StaffNubmer " class="form-control" />
    <br />
    <button class="btn btn-primary"> Personeli Ekle </button>
</form>



